/* pages/myCommission/myCommission.wxss */
.my_commission{

}
.my_commission .header{
  margin: 0 auto 60rpx;
  width:670rpx;
  height:380rpx;
   border-radius:20rpx;
  background:#0b99fb;
  box-shadow:0 2px 8px 0 rgba(89,143,180,0.50);
  text-align: center;
  color:#ffffff;
}
.my_commission .header .title{
  padding-top: 60rpx;
  font-size:32rpx;
}
.my_commission .header .money{
  padding-top: 20rpx;
  font-size:90rpx;
}
.my_commission .header .info{
  padding-top: 32rpx;
}
.my_commission .header .info text{
  font-size:26rpx;
  padding: 10rpx 20rpx;
  background:rgba(0,0,0,0.1);
  border-radius: 29rpx;
}
.commission_list .list{
  width:662rpx;
  height:140rpx;
  padding: 0 30rpx;
  background:#ffffff;
  box-shadow:0 4rpx 40rpx 0 rgba(215,215,215,0.60);
  border-radius:8rpx;
  margin: 0 auto 40rpx;
  display: flex;
  display: -webkit-flex;
  justify-content: space-between; 
  align-items: center;
  font-size:32rpx;
  color:#0c1b37;
}
.commission_list .list .left{
  display: flex;
  display: -webkit-flex;
  align-items: center;
}
.commission_list .list image{
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
}
.commission_list .list .right text:nth-of-type(1){
  font-size:36rpx;
}
.commission_list .list .right text:nth-of-type(2){
  font-size:56rpx;
}